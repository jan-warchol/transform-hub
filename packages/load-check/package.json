{
  "name": "@scramjet/load-check",
  "version": "0.16.0",
  "description": "This package is part of Scramjet Transform Hub. The package provides health info for STH.",
  "main": "./src/index.ts",
  "scripts": {
    "start": "ts-node ./src/index",
    "build": "tsc -p tsconfig.build.json",
    "build:docs": "typedoc",
    "clean": "rm -rf ./dist .bic_cache",
    "cloc": "cloc src --fullpath --include-lang TypeScript --not-match-d \"(node_modules|test|dist|bdd)\" --by-percent cm",
    "test": "nyc ava",
    "prepack": "node ../../scripts/publish.js",
    "postbuild": "yarn prepack"
  },
  "author": "Scramjet <open-source@scramjet.org>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@scramjet/model": "^0.16.0",
    "@scramjet/obj-logger": "^0.16.0",
    "@scramjet/sth-config": "^0.16.0",
    "@scramjet/symbols": "^0.16.0",
    "@scramjet/utility": "^0.16.0",
    "scramjet": "^4.36.1",
    "systeminformation": "^5.7.10",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@scramjet/types": "^0.16.0",
    "@types/node": "^17.0.18",
    "@types/uuid": "^8.3.1",
    "ava": "^4.0.1",
    "ts-node": "^10.0.0",
    "typedoc": "^0.22.11",
    "typedoc-plugin-markdown": "^3.11.14",
    "typescript": "^4.3.4"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "**/*.spec.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scramjetorg/transform-hub.git"
  }
}
